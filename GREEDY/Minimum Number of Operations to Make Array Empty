class Solution {
public:
    int minOperations(vector<int>& nums) {
        unordered_map<int, int> mp;
        for(auto &it : nums){
            mp[it]++;
        }
        int ops = 0;
        for(auto &it : mp){
            int freq = it.second;
            if(freq == 1) return -1;
            ops += freq / 3;
            if(freq % 3 != 0) ops++;
        }
        return ops;
    }
};
