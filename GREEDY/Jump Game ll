class Solution {
public:
    int jump(vector<int>& nums) {
        int n = nums.size();
        int jumps = 0;
        int start = 0, end = 0;
        while(end < n-1){
            int maxReach = 0;
            for(int i = start; i <= end; i++) maxReach = max(maxReach, i + nums[i]);
            start = end+1;
            end = maxReach;
            jumps++;
        }
        return jumps;
    }
};
