class Solution {
public:
    int minimumRounds(vector<int>& tasks) {
        unordered_map<int, int> mp;
        for(auto &it : tasks){
            mp[it]++;
        }
        int rounds = 0;
        for(auto &it : mp){
            int freq = it.second;
            if(freq == 1) return -1;
            rounds += freq / 3;
            if(freq % 3 != 0) rounds++;
        }
        return rounds;
    }
};
