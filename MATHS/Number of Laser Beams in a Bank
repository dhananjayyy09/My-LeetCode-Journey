class Solution {
public:
    int numberOfBeams(vector<string>& bank) {
        int ans = 0, prev = 0;
        for(auto &b : bank){
            int curr = count(b.begin(), b.end(), '1');
            if(curr > 0){
                ans += curr * prev;
                prev = curr;
            }
        }
        return ans;
    }
};
