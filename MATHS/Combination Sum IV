class Solution {
public:
    int combinationSum4(vector<int>& nums, int target) {
        int n = nums.size();
        vector<long long> dp(target + 1, 0);
        dp[0] = 1;
        for(int t=1; t <= target; t++){
            for(int num : nums){
                if(num <= t){
                    dp[t] += dp[t - num];
                    if (dp[t] > INT_MAX) dp[t] = INT_MAX;  
                }
            }
        }
        return (int)dp[target];
    }
};
