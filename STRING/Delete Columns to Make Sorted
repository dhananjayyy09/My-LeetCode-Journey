class Solution {
public:
    int minDeletionSize(vector<string>& s) {
        int m = s.size(), n = s[0].size();
        int count = 0;
        for(int j=0; j<n; j++){
            for(int i=1; i<m; i++){
                if(s[i][j] < s[i-1][j]){
                    count++;
                    break;
                } 
            }
        }
        return count;
    }
};
