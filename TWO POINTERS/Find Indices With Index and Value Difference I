class Solution {
public:
    vector<int> findIndices(vector<int>& nums, int indexDiff, int valueDiff) {
        int n = nums.size();
        for (int i = 0; i < n; i++) {
            for (int j = i + indexDiff; j < n; j++) {
                if (abs(nums[i] - nums[j]) >= valueDiff) {
                    return {i, j};
                }
            }
        }
        return {-1, -1};
        }
};
