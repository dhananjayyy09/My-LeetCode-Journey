class Solution {
public:
    int maximizeGreatness(vector<int>& nums) {
        int n = nums.size();
        sort(nums.begin(), nums.end());
        int greatness = 0;
        int i = 0, j = 0;
        while(j < n){
            if(nums[i] < nums[j]){
                greatness++;
                i++;
            }
            j++;
        }
        return greatness;
    }
};
