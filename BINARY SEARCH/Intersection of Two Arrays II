class Solution {
public:
    vector<int> intersect(vector<int>& nums1, vector<int>& nums2) {
        unordered_map<int,int> mp;
        vector<int> ans;
        for(auto n1 : nums1){
            mp[n1]++;
        }
        for(auto n2 : nums2){
            if(mp[n2] > 0){
                ans.push_back(n2);
                mp[n2] --;
            }
        }
        return ans;
    }
};
