class Solution {
public:
    int longestSquareStreak(vector<int>& nums) {
        int n = nums.size();
        unordered_set<int> st(nums.begin(), nums.end());
        int ans = -1;
        for(auto &it : nums){
            int x = sqrt(it);
            if(st.count(x)){
                ans++;
            }
        }
        return ans >= 2 ? ans : -1;
    }
};
