class Solution {
public:
    int divisorSubstrings(int num, int k) {
        int beauty = 0;
        string n = to_string(num);
        for(int i=0; i <= n.size()-k; i++){
            string str = n.substr(i,k);
            int v = stoi(str);
            if(v != 0 && num % v == 0){
                beauty++;
            } 
        }
        return beauty;
    }
};
