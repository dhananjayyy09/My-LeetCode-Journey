class Solution {
public:
    int maxProduct(vector<int>& nums) {
        int n = nums.size();
        int maxPro = nums[0];
        int curMax = nums[0], curMin = nums[0];
        for(int i=1; i<n; i++){
            if(nums[i] < 0) swap(curMax, curMin);
            curMax = max(nums[i], nums[i] * curMax);
            curMin = min(nums[i], nums[i] * curMin);
            maxPro = max(maxPro, curMax);
        }
        return maxPro;
    }
};
